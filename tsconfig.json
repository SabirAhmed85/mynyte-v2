{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["components/*"],
      "@config/*": ["config/*"],
      "@constants/*": ["constants/*"],
      "@hooks/*": ["hooks/*"],
      "@models": ["models"],
      "@types/*": ["types/*"]
    },
    "allowSyntheticDefaultImports": true,
    "typeRoots": [ "./types", "./node_modules/@types"],
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true
  }
}
