{
    "expo": {
        "name": "MyNyte (All-in-1 Evening-Planning App)",
        "slug": "mynyte-app",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "mynyte",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#555555"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.mynyte.mynyte"
        },
        "web": {
            "favicon": "./assets/images/favicon.png"
        },
        "android": {
            "package": "com.mynyte.mynyte",
            "intentFilters": [{
                "action": "VIEW",
                "autoVerify": true,
                "data": [{
                    "scheme": "https",
                    "host": "*.mynyte.io",
                    "pathPrefix": "/records"
                }],
                "category": [
                    "BROWSABLE",
                    "DEFAULT"
                ]
            }]
        }
    }
}